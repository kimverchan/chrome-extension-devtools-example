{
  "action": {
    "default_icon": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "default_title": "DevTools-Example"
  },
  "content_scripts": [],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "description": "DevTools browser extension example",
  "devtools_page": "pages/devtools-background.html",
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "manifest_version": 3,
  "name": "DevTools-Example",
  "permissions": ["scripting"],
  "version": "1.0.0.0",
  "version_name": "1.0.0.0",
  "web_accessible_resources": [
    {
      "extension_ids": [],
      "matches": ["<all_urls>"],
      "resources": ["dist/*", "client/*", "overlay/*", "pages/*"]
    }
  ]
}
